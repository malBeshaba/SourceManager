/* pages/cart/orderList/orderList.wxss */
form {
  width: 100%;
  height: 100%;
  background-color: #eeeeee;
}
.source_info {
  width: 100%;
  height: 20%;
  display: flex;
  flex-direction: row;
  background-color: white;
}
.div-line{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
}
.source_info image {
  height: 90%;
  width: 90px;
}

.source_info text {
  text-align: center;
  margin-top: 10%;
  margin-left: 20%;
}

.section_gap {
  margin-top: 31px;
  background-color: white;
  border-top-right-radius: 10px;
  border-top-left-radius: 10px;
  display: flex;
  flex-direction: column;
}
.section_gap checkbox-group {
  display: flex;
  flex-direction: column;
}

.checkbox {
  margin-top: 10px;
  margin-bottom: 10px;
}